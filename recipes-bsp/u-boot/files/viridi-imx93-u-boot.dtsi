&gpio1 {
	compatible = "fsl,imx93-gpio", "fsl,imx7ulp-gpio";
	reg = <0x47400080 0x1000>, <0x47400040 0x40>;
	bootph-pre-ram;
	bootph-some-ram;
};

&gpio2 {
	compatible = "fsl,imx93-gpio", "fsl,imx7ulp-gpio";
	reg = <0x43810080 0x1000>, <0x43810040 0x40>;
	bootph-pre-ram;
	bootph-some-ram;
};

&gpio3 {
	compatible = "fsl,imx93-gpio", "fsl,imx7ulp-gpio";
	reg = <0x43820080 0x1000>, <0x43820040 0x40>;
	bootph-pre-ram;
	bootph-some-ram;
};

&gpio4 {
	compatible = "fsl,imx93-gpio", "fsl,imx7ulp-gpio";
	reg = <0x43830080 0x1000>, <0x43830040 0x40>;
	bootph-pre-ram;
	bootph-some-ram;
};

