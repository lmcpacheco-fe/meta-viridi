	pinctrl_lpi2c5: lpi2c5grp {
		fsl,pins = <
			MX91_PAD_GPIO_IO22__LPI2C5_SDA		0x40000b9e
			MX91_PAD_GPIO_IO23__LPI2C5_SCL		0x40000b9e
		>;
	};

	pinctrl_pca9555_20: pca9555_20grp {
		fsl,pins = <
			MX91_PAD_PDM_CLK__GPIO1_IO8		0x31e
